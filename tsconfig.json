{
	"extends": "astro/tsconfigs/base",
	"compilerOptions": {
		"baseUrl": ".",
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"paths": {
			"@/*": ["./*"],
			"@components/*": ["src/components/*"],
			"@layouts/*": ["src/layouts/*"],
			"@assets/*": ["src/assets/*"],
			"@data/*": ["src/data/*"],
			"@type/*": ["src/types/*"]
		},
		"types": ["vite/client"],
		"plugins": [
			{
				"name": "@astrojs/ts-plugin"
			}
		]
	},
	"include": ["src", "styled-system"]
}
